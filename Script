local teleportService = game:GetService("TeleportService")
local players = game:GetService("Players")
local localPlayer = players.LocalPlayer

-- Hedef oyun ID'si
local targetGameId = 16670640252

-- Teleport fonksiyonu
local function teleportToTargetGame()
    teleportService:Teleport(targetGameId, localPlayer)
end

-- İlk teleport
teleportToTargetGame()

-- Geri geldikten sonra tekrar teleport etmek için bir kontrol
local function checkReturn()
    while true do
        wait(9) -- 9 saniye bekle
        teleportToTargetGame() -- Tekrar teleport et
    end
end

-- Oyuncunun karakteri yüklendiğinde kontrolü başlat
localPlayer.CharacterAdded:Connect(function()
    checkReturn()
end)
